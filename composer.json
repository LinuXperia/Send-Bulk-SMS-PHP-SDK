{
        "name": "smsgatewaycenterphpsdk/smsgatewaycenterapi",
        "type": "library",
        "description": "SMS Gateway Center PHP SDK for API",
        "keywords": ["sms gateway center php sdk", "sms gateway center api integration", "bulk sms api"],
        "homepage": "https://github.com/smsgatewaycenter/smsgatewaycenter-php-sdk",
        "license": "MIT",
        "authors": [
                {
                        "name": "Milind Gunji",
                        "email": "contact@smsgatewaycenter.com",
                        "role": "Developer"
                }
        ],

        "require": {
                "php": ">=5.3.0",
                "guzzlehttp/guzzle": "^6.3"
        },
        "autoload": {
                "psr-4": {
                        "SMSGatewayCenterPhpSdk\\object\\": ["src/object/common", "src/object/reseller", "src/object/sms", "src/object/user"],
                        "SMSGatewayCenterPhpSdk\\config\\": ["src/config/common", "src/config/reseller", "src/config/sms", "src/config/user", "src/config/reports"],
                        "SMSGatewayCenterPhpSdk\\api\\user\\": ["src/api/user"],
                        "SMSGatewayCenterPhpSdk\\api\\sms\\": ["src/api/sms"],
                        "SMSGatewayCenterPhpSdk\\api\\reseller\\": ["src/api/reseller"],
                        "SMSGatewayCenterPhpSdk\\api\\info\\": ["src/api/info"],
                        "SMSGatewayCenterPhpSdk\\api\\reports\\": ["src/api/reports"],
                        "SMSGatewayCenterPhpSdk\\lib\\": ["src/lib"]
                }
        },
	"minimum-stability": "dev",
	"prefer-stable": true
}
